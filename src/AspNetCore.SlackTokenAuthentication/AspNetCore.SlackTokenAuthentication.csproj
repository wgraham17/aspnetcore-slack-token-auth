<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>https://github.com/wgraham17</Authors>
    <Company>https://github.com/wgraham17</Company>
    <Product>Slack Token Authentication for ASP.NET Core</Product>
    <Description>An ASP.NET Core authentication provider for Slack slash command and interactive component tokens.</Description>
    <PackageProjectUrl>https://github.com/wgraham17/aspnetcore-slack-token-auth</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/wgraham17/aspnetcore-slack-token-auth/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/wgraham17/aspnetcore-slack-token-auth</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>authentication slack aspnetcore</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Authentication" Version="2.0.3" />
  </ItemGroup>

</Project>
